#include "number.grammar"
#include "number.cn.grammar"

// for local grammars that come from regex tagger
<number> = <numeric> {nf.math.to_number($1)};

<result> = <number>  "plus" /*comments*/<number> {nf.math.sum($1,$3)}
         | "sum of" <number> "and" <number> {nf.math.sum($2,$4)}
         | <number> "与" <number> "的和" {nf.math.sum($1,$3)}
         ;


